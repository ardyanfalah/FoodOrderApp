<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="560dp"
    android:layoutDirection="ltr">

    <View
        android:id="@+id/view2"
        android:layout_width="100dp"
        android:layout_height="200dp"
        android:background="#95D6E6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="100dp"
        android:layout_height="240dp"
        android:background="#95D6E6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view4"
        android:layout_width="220dp"
        android:layout_height="60dp"
        android:background="#95D6E6"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="80dp"
        android:layout_height="match_parent"
        android:background="#9DE695"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/v_meja_1"
        android:layout_width="60dp"
        android:layout_height="100dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="40dp"
        android:background="#9A9A9A"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <View
        android:id="@+id/view11"
        style="@style/Divider"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toTopOf="@+id/v_meja_2"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/v_meja_1" />

    <View
        android:id="@+id/view10"
        style="@style/Divider"
        android:layout_marginEnd="20dp"
        app:layout_constraintBottom_toTopOf="@+id/v_meja_3"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/v_meja_2" />

    <View
        android:id="@+id/v_meja_2"
        android:layout_width="60dp"
        android:layout_height="100dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="40dp"
        android:background="#9A9A9A"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/v_meja_1" />

    <View
        android:id="@+id/v_meja_3"
        android:layout_width="60dp"
        android:layout_height="100dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="40dp"
        android:background="#9A9A9A"
        app:layout_constraintEnd_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/v_meja_2" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sawah"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="@+id/view3" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Empang"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Empang"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meja 1"
        android:textColor="@color/color_white"
        app:layout_constraintBottom_toBottomOf="@+id/v_meja_1"
        app:layout_constraintEnd_toEndOf="@+id/v_meja_1"
        app:layout_constraintStart_toStartOf="@+id/v_meja_1"
        app:layout_constraintTop_toTopOf="@+id/v_meja_1" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meja 2"
        android:textColor="@color/color_white"
        app:layout_constraintBottom_toBottomOf="@+id/v_meja_2"
        app:layout_constraintEnd_toEndOf="@+id/v_meja_2"
        app:layout_constraintStart_toStartOf="@+id/v_meja_2"
        app:layout_constraintTop_toTopOf="@+id/v_meja_2" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Meja 3"
        android:textColor="@color/color_white"
        app:layout_constraintBottom_toBottomOf="@+id/v_meja_3"
        app:layout_constraintEnd_toEndOf="@+id/v_meja_3"
        app:layout_constraintStart_toStartOf="@+id/v_meja_3"
        app:layout_constraintTop_toTopOf="@+id/v_meja_3" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="4dp"
        android:text="Masuk"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view8" />

    <TextView
        android:id="@+id/tv_to_saung2"
        android:layout_width="64dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Saung 2"
        app:layout_constraintBottom_toTopOf="@+id/view8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />
</androidx.constraintlayout.widget.ConstraintLayout>