<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="viewModel"
            type="e.ardya.foodorderapp.viewmodel.HomeViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="560dp"
        android:layoutDirection="ltr">

        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="#95D6E6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/view12"
            android:layout_width="match_parent"
            android:layout_height="240dp"
            android:background="#95D6E6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <View
            android:id="@+id/view14"
            android:layout_width="120dp"
            android:layout_height="0dp"
            android:background="#95D6E6"
            app:layout_constraintBottom_toTopOf="@+id/view12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view9" />

        <View
            android:id="@+id/view13"
            android:layout_width="80dp"
            android:layout_height="0dp"
            android:background="#95D6E6"
            app:layout_constraintBottom_toTopOf="@id/guideline8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view9" />

        <View
            android:id="@+id/view19"
            android:layout_width="80dp"
            android:layout_height="0dp"
            android:background="#95D6E6"
            app:layout_constraintBottom_toTopOf="@+id/view12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/guideline9" />

        <View
            android:id="@+id/v_meja_4"
            android:layout_width="60dp"
            android:layout_height="100dp"
            android:background="#9A9A9A"
            app:layout_constraintBottom_toTopOf="@+id/view12"
            app:layout_constraintEnd_toStartOf="@+id/view13"
            app:layout_constraintStart_toEndOf="@+id/view14"
            app:layout_constraintTop_toBottomOf="@+id/view9" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Meja 4"
            android:textColor="@color/color_white"
            app:layout_constraintBottom_toBottomOf="@+id/v_meja_4"
            app:layout_constraintEnd_toEndOf="@+id/v_meja_4"
            app:layout_constraintStart_toStartOf="@+id/v_meja_4"
            app:layout_constraintTop_toTopOf="@+id/v_meja_4" />

        <TextView
            android:id="@+id/tv_to_saung1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="Saung 1"
            app:layout_constraintBottom_toTopOf="@+id/view19"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view13" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.45" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>